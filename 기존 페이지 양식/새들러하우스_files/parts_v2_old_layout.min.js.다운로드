function alert_popup(n){$("#"+n).addClass("popup_show");$("body").addClass("body_fixed")}function refresh(){return location.reload(),location.href}function login(){if(g_webapp_yn=="Y")location.href=stringFormat("/{0}login?return_url={1}",g_route_char,window.location.href);else{var n=stringFormat("/{0}login?return_url={1}",g_route_char,window.location.href);location.href=n}}function join(){location.href=stringFormat("/{0}signup",g_route_char)}function logout(){if(confirm("로그아웃 하시겠습니까?")){$(".btn_close_aside").click();var n=stringFormat("/{0}logout",g_route_char);location.href=n}}function mypage(){if(user_idx==0){var n=stringFormat("/{0}login?return_url={1}",g_route_char,window.location.href);location.href=n}else location.href=stringFormat("/{0}me/pages",g_route_char)}function cart(){if(user_idx==0){var n=stringFormat("/{0}login?return_url={1}",g_route_char,window.location.href);location.href=n}else location.href=stringFormat("/{0}carts",g_route_char)}function back(){window.history.length>0?window.history.back():location.href=stringFormat("/{0}",g_route_char)}function doSearching(n){if(n=="")return alert("검색어를 입력해 주세요."),!1;location.href=stringFormat("/{0}find?search_text=",g_route_char)+n}function move_to_product(n){var t;g_webapp_yn=="Y"?(t=stringFormat("{2}/{0}products/{1}",g_route_char,n,g_domain),callNative("NaviPushWithUrl",t)):(t=stringFormat("/{0}products/{1}",g_route_char,n),location.href=t)}function move_to_url(n){if(g_webapp_yn=="Y"){var n=stringFormat("{0}{1}",g_domain,n);callNative("NaviPushWithUrl",n)}else location.href=n}function adultCertification(){g_user_idx>0?($("#popup_auth_adult").addClass("popup_show"),$(".wrap").hide(),$("body").addClass("body_fixed")):(alert("로그인을 먼저 해주세요."),location.href=stringFormat("/{0}login",g_root_dir))}$(function(){$("#btn_login").click(function(n){n.preventDefault();login()});$("#btn_left_back").click(function(n){n.preventDefault();g_webapp_yn=="Y"?callNative("HistoryBack",""):history.back()});$("#btn_login_back").click(function(n){n.preventDefault();g_webapp_yn=="Y"?location.href=stringFormat("/{0}",g_route_char):history.back()});$("#btn_right_search").click(function(n){n.preventDefault();location.href=stringFormat("/{0}find",g_route_char)});$("#btn_layer_logout").click(function(){alert_popup("logout_popup")});$(".btn_cls_pop,.popup .popup_bg,.btn_txt_cls_pop").click(function(){$(this).parents(".popup").removeClass("popup_show");$("body").removeClass("body_fixed")});$("#btn_ok_logout").click(function(n){n.preventDefault();localStorage.removeItem("token");location.href=stringFormat("/{0}logout",g_route_char)});$("#btn_right_home").click(function(n){n.preventDefault();location.href=stringFormat("/{0}",g_route_char)});$("#btn_navi_back").click(function(n){n.preventDefault();g_webapp_yn=="Y"?callNative("NaviPopWithData","aaa=bbb&ccc=ddd"):history.back()});$("#btn_home").click(function(n){n.preventDefault();g_webapp_yn=="Y"?callNative("NaviPopToRoot",""):history.back()});g_webapp_yn=="Y"&&callNative("CurrentPage",window.location.href)});