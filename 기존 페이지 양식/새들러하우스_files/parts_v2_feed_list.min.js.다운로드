function move_history_back(n){$("html, body").animate({scrollTop:n},0)}function redirect_blank(n){var t=document.createElement("a");t.target="_blank";t.href=n;t.click()}var offset_margin_top=0;$(function(){var t,n,r,u,i;if(g_cell_phone_auth_yn=="N"&&(location.href="/-v2/logout"),g_opening_popup=="Y"&&g_feed_use_yn=="Y"){getCookie("shop_popup")!="Y"&&($("#popup_notice").addClass("popup_show"),$("body").addClass("body_fixed"));$("#close_today").click(function(n){n.preventDefault();setCookie("shop_popup","Y",1);$(".btn_cls_pop,.popup_bg").click()});$(".btn_cls_pop,.popup_bg").on("click",function(){$(".popup").removeClass("popup_show");$("body").removeClass("body_fixed")});t=$(".popup_swiper .swiper-slide").length;n="";n=t>1?{disableOnInteraction:!1,slidesPerView:1,spaceBetween:0,speed:400,loop:!0,autoplay:{delay:3e3,disableOnInteraction:!1},pagination:{el:".swiper-pagination"}}:{init:!1};r=new Swiper(".popup_swiper",n)}u=new Swiper(".home_visual",{slidesPerView:1,scrollbar:{el:".home_visual .swiper-scrollbar",hide:!1},autoplay:{delay:2500,disableOnInteraction:!1}});$(".item_swipe").each(function(n){var t=$(this),i;t.addClass("instance-"+n);i=new Swiper(".instance-"+n,{slidesPerView:"auto",spaceBetween:20})});$(".fade_up_content").waypoint(function(n){n==="down"&&$(this.element).addClass("fade_up")},{offset:"100%"});$(".btn_cls_pop,.popup .popup_bg").click(function(){$(this).parents(".popup").removeClass("popup_show");$("body").removeClass("body_fixed");$("#banner_title").empty();$("#banner_desc").empty();$("#feed_card_img").empty();$("#feed_body_img").empty()});offset_margin_top=$(".header_fxd_wrap").height();$(document).on("click","a[id^='f_'].link_view_feed",function(n){n.preventDefault();var t=$(this),i=t.offset().top-offset_margin_top,r=$(this).attr("banner_idx"),u=stringFormat("banner_idx={0}",r);$.ajax({type:"GET",url:url_path+"/banner/get",data:u,dataType:"json",cache:!1,success:function(n){var f,l;if(n.response_code=="1000"&&n.response_data_count>0){var a=n.response_data,t=a[0],e=t.banner_idx,o=t.banner_title,s=t.banner_desc,h=t.banner_content,u=t.click_move_type_idx,r=t.banner_url,c=t.popup_yn;u==1?(o.length>0||s.length>0?($("#banner_title").empty().append(o),$("#banner_desc").empty().append(s),$("#feed_card_img").empty().append(h),r.length>0&&(f=r.split(","),l=stringFormat('<div style="margin-top:20px"><a href="{1}" class="btn btn_t1 btn_inline md-ripples">{0}<\/a><\/div>',f[0],f[1]),$("#banner_desc").append(l)),$("#popup_feed_body").hide(),$("#feed_card_img").show(),$("#popup_body").show()):($("#feed_body_img").empty().append(h),$("#popup_feed_body").show(),$("#feed_card_img").hide(),$("#popup_body").hide()),$("#feed_popup").addClass("popup_show"),$("body").addClass("body_fixed")):u==2?c=="Y"?redirect_blank(stringFormat("/{0}feed/content/{1}",g_route_char,e)):(localStorage.feed_offset_position=i,location.href=stringFormat("/{0}feed/content/{1}",g_route_char,e)):u==3&&(c=="Y"?redirect_blank(r):location.href=r)}}})});g_instagram_token!=""&&g_instagram_yn=="Y"&&(i=new Instafeed({accessToken:g_instagram_token,target:"instagram_row",limit:9,template:'<div class="instargram_item"><a href="{{link}}" target="_blank"><span style="background-image:url({{image}})"><\/span><\/a><\/div>'}),i.run());g_youtube_playlist_use_yn=="Y"&&$.ajax({type:"GET",url:url_path+"/test/youtube",data:"",dataType:"json",cache:!1,success:function(n){var t=n.items;$.each(t,function(n,t){var i=t.snippet.thumbnails.medium.url,r=t.snippet.title,u=t.snippet.resourceId.videoId,f='<li>                    <div class="ytd_box">                        <a class="ytb_link" aria-hidden="true" tabindex="-1" rel="null" href="https://www.youtube.com/watch?v='+u+'}" target="_blank">                            <div class="ytd_thumbnail"><img class="ytb_img" alt="" src="'+i+'"><\/div>                            <div class="ytb_title ellip_line2">'+r+"<\/div>                        <\/a>                    <\/div>                <\/li>";$("#youtube_row").append(f)})}})});window.onpageshow=function(n){var t;t=localStorage.feed_offset_position!=undefined?localStorage.feed_offset_position:0;n.persisted||window.performance&&window.performance.navigation.type==2?move_history_back(t):move_history_back(t);localStorage.removeItem("feed_offset_position")};