var isIOS=(function(){var iosQuirkPresent=function(){var audio=new Audio();audio.volume=0.5;return audio.volume===1;};var isIOS=/iPad|iPhone|iPod/.test(navigator.userAgent);var isAppleDevice=navigator.userAgent.includes('Macintosh');var isTouchScreen=navigator.maxTouchPoints>=1;return isIOS||(isAppleDevice&&(isTouchScreen||iosQuirkPresent()));})();